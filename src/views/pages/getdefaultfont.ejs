<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>System Default Font</title>
    <style>
    </style>
</head>
<body>
    <div id="defaultFontElement" class="default-font">你媽</div>
    <script>
        function getSystemDefaultFont() {
            const element = document.getElementById('defaultFontElement');
            const computedStyle = window.getComputedStyle(element);
            const fontFamily = computedStyle.getPropertyValue('font-family');
            return fontFamily;
        }

        console.log('System Default Font:', getSystemDefaultFont());
    </script>
</body>
</html>
